<view class="weui_cell" bindtap='ToPunch'>
  <image class="img" src="../../image/myPageIcon/clockIn.png"> </image>
  <view class="weui_cell_bd_p">打卡</view>
</view>

<view class="weui_cell" bindtap='ToHistoryScheduleList'>
  <image class="img" src="../../image/myPageIcon/history.png"> </image>
  <view class="weui_cell_bd_p">查看历史训练计划</view>
</view>

</view>